{
    "name": "getargv-gh-pages",
    "version": "0.0.1",
    "description": "Marketing site for getargv",
    "main": "index.js",
    "scripts": {
        "start": "(trap 'kill 0' SIGINT; sass --watch assets/sass/main.scss assets/css/main.css & /usr/bin/python -m SimpleHTTPServer)",
        "build": "sass assets/sass/main.scss assets/css/main.css",
        "gitlog": "git log master --oneline",
        "mandoc": "git show master:man/getargv.1 | mandoc -T html -O style=mandoc.css > man.html && /usr/bin/curl -sSL https://mandoc.bsd.lv/snapshots/mandoc.tar.gz | /usr/bin/tar -x --strip-components=1 --include='*mandoc.css' --file -",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Camden Narzt",
    "license": "SEE LICENSE IN LICENSE.txt",
    "dependencies": {
        "sass": "^1.32.11"
    }
}
